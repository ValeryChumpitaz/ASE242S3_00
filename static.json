{
  "headers": [
    {
      "source": "**/*.@(js|css)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/api/*",
      "destination": "/api/"
    }
  ],
  "rewrites": [
    {
      "source": "/**",
      "destination": "/index.html"
    }
  ]
}
